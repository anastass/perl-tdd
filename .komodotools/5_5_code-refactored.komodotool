    "!@#_anchorpackage Text::Para;", 
    "", 
    "use Moose;", 
    "", 
    "has 'columns' => (is => 'rw', isa => 'Int', default => 72);", 
    "", 
    "sub format {", 
    "    my($self, $text) = @_;", 
    "", 
    "    my @words = $text =~ m/(\\S+)/g;", 
    "    return join ' ', @words;", 
    "}", 
    "", 
    "1;!@#_currentPos"
